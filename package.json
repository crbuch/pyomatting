{
  "name": "pyomatting",
  "version": "1.3.6",
  "description": "TypeScript package for closed-form alpha matting using Pyodide - bringing Python computer vision to the browser with memory-efficient TypedArray processing",
  "type": "module",
  "main": "dist/pyomatting.es.js",
  "module": "dist/pyomatting.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "example": "cd examples && npm run dev",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pyodide", 
    "python", 
    "typescript", 
    "wasm", 
    "alpha-matting", 
    "computer-vision",
    "image-processing",
    "matting",
    "foreground-extraction",
    "webassembly",
    "browser",
    "canvas",
    "imagedata"
  ],
  "author": "crbuch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crbuch/pyomatting.git"
  },
  "homepage": "https://github.com/crbuch/pyomatting",
  "bugs": {
    "url": "https://github.com/crbuch/pyomatting/issues"
  },
  "dependencies": {
    "pyodide": "^0.26.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5",
    "vite": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16"
  }
}
